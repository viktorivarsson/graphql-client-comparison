{
  "name": "relay-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn relay && vite",
    "relay": "yarn run relay-compiler --language typescript --watchman false $@",
    "build": "yarn run relay && tsc && vite build",
    "serve": "vite preview",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:4000/graphql schema.graphql"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-relay": "^12.0.0",
    "react-router-dom": "^5.3.0",
    "relay-runtime": "^12.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-relay": "^11.0.2",
    "@types/react-router-dom": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.0",
    "apollo": "^2.33.6",
    "babel-plugin-relay": "^12.0.0",
    "graphql": "^15.6.1",
    "prettier": "^2.4.1",
    "relay-compiler": "^12.0.0",
    "relay-compiler-language-typescript": "^14.2.1",
    "relay-config": "^12.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.0",
    "vite-plugin-relay": "^1.0.6"
  }
}
